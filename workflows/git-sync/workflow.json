        410
      ]
    },
    {
      "parameters": {
        "functionCode": "const apiBase = ($env.N8N_API_URL || 'https://n8n.oremuslabs.app').replace(/\\/$/, '');\nconst sourceItems = $items(\"Select Workflow Files\", 0) || [];\nconst inputItems = $input.all();\n\nreturn inputItems.map((workflowItem, index) => {\n  const workflow = workflowItem.json;\n  const path = sourceItems[index]?.json?.path || 'workflows/unknown/workflow.json';\n  const workflowId = workflow.id;\n  const hasValidId = typeof workflowId === 'string' && workflowId.trim() !== '';\n  const cleanedWorkflow = {\n    name: workflow.name,\n    nodes: workflow.nodes,\n    connections: workflow.connections,\n    settings: workflow.settings ?? {}\n  };\n\n  return {\n    json: {\n      hasValidId,\n      workflowId,\n      apiBase,\n      body: cleanedWorkflow,\n      sourcePath: path,\n      workflowName: workflow.name || 'Unnamed Workflow'\n    }\n  };\n});"
      },
