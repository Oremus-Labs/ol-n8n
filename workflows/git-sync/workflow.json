      "position": [
        740,
        410
      ]
    },
    {
      "parameters": {
        "functionCode": "const tree = $json.tree || [];\nconst workflows = tree\n  .filter(entry => entry.type === 'blob' && /^workflows\\/[^\\/]+\\/workflow\\.json$/.test(entry.path))\n  .map(entry => ({\n    path: entry.path,\n    rawUrl: `https://raw.githubusercontent.com/Oremus-Labs/ol-n8n/main/${entry.path}`\n  }));\nreturn workflows.map(item => ({ json: item }));"
      },
      "id": "selectWorkflows",
      "name": "Select Workflow Files",
      "type": "n8n-nodes-base.function",
      "typeVersion": 1.3,
      "position": [
        980,
        410
      ]
    },
    {
      "parameters": {
        "url": "={{$json.rawUrl}}",
        "responseFormat": "json",
        "authentication": "none",
        "headerParameters": {
          "parameter": [
            {
              "name": "Accept",
              "value": "application/json"
            }
          ]
        },
        "options": {
          "onError": "continueRegularOutput"
        }
      },
      "id": "fetchWorkflow",
      "name": "Fetch Workflow JSON",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.3,
      "position": [
        1220,
        410
      ]
    },
    {
      "parameters": {
        "functionCode": "// Get the workflow from previous node (Fetch Workflow JSON)\nconst workflow = $json;\nconst prevPath = $('Select Workflow Files').item.json.path;\n\n// Extract workflow info\nconst path = prevPath || 'workflows/unknown/workflow.json';\nconst apiBase = ($env.N8N_API_URL || 'https://n8n.oremuslabs.app').replace(/\\/$/, '');\n\n// Determine if update or create\nconst workflowId = workflow.id;\nconst hasValidId = typeof workflowId === 'string' && workflowId.trim() !== '';\n\nconst method = hasValidId ? 'PUT' : 'POST';\nconst url = hasValidId ? `${apiBase}/api/v1/workflows/${workflowId}` : `${apiBase}/api/v1/workflows`;\n\n// Clean the workflow - remove runtime-only fields\nconst cleanedWorkflow = { ...workflow };\ndelete cleanedWorkflow.createdAt;\ndelete cleanedWorkflow.updatedAt;\ndelete cleanedWorkflow.versionId;\n\nreturn {\n  json: {\n    method,\n    url,\n    body: cleanedWorkflow,\n    sourcePath: path,\n    workflowName: workflow.name || 'Unnamed Workflow'\n  }\n};"
      },
      "id": "normalizeWorkflow",
      "name": "Prepare API Request",
      "type": "n8n-nodes-base.function",
      "typeVersion": 1.3,
