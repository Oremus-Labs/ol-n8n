    {
      "parameters": {
        "functionCode": "const tree = $json.tree || [];\nconst workflows = tree\n  .filter(entry => entry.type === 'blob' && /^workflows\\/[^\\/]+\\/workflow\\.json$/.test(entry.path))\n  .map(entry => ({\n    path: entry.path,\n    rawUrl: `https://raw.githubusercontent.com/Oremus-Labs/ol-n8n/main/${entry.path}`\n  }));\nreturn workflows.map(item => ({ json: item }));"
      },
